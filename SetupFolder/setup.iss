[Setup]
; NOTE: The value of AppId uniquely identifies this application.
; Do not use the same AppId value in installers for other applications.
; (To generate a new GUID, click Tools | Generate GUID inside the IDE.)
AppId={{79A86AD1-39B0-47A8-A497-89B77A40C4E5}
AppName=Mutimeter Simulator
AppVersion=1.0
;AppVerName=Mutimeter Simulator 1.0
AppPublisher=IoT Solutions Company
AppPublisherURL=http://www.iotsolutions.vn
AppSupportURL=http://www.iotsolutions.vn
AppUpdatesURL=http://www.iotsolutions.vn
DefaultDirName={pf}\Mutimeter Simulator
DisableProgramGroupPage=yes
LicenseFile=F:\WorkSpace\MultimeterSimulator\SetupFolder\License.txt
