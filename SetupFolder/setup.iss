Name: "{commondesktop}\Mutimeter Simulator"; Filename: "{app}\MultimeterSimulator.exe"; Tasks: desktopicon

[Run]
Filename: "{app}\MultimeterSimulator.exe"; Description: "{cm:LaunchProgram,Mutimeter Simulator}"; Flags: nowait postinstall skipifsilent

